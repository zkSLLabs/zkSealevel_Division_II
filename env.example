RPC_URL=https://api.devnet.solana.com
WS_URL=wss://api.devnet.solana.com
PROGRAM_ID_VALIDATOR_LOCK=
ZKSL_MINT=
CHAIN_ID=103
MIN_FINALITY_COMMITMENT=finalized
# Devnet-only deployment; v2 and STARK disabled
# For Testnet (per Operation-Ghost-Ship):
# RPC_URL=https://api.testnet.solana.com
# WS_URL=wss://api.testnet.solana.com
# CHAIN_ID=102

ARTIFACT_DIR=./orchestrator/data/artifacts
AGGREGATOR_KEYPAIR_PATH=./keys/aggregator.example.json
FEE_PAYER_KEYPAIR_PATH=~/.config/solana/id.json

PORT=8080

DATABASE_URL=postgres://postgres:postgres@localhost:5432/zksl
# For production, use strong credentials and TLS, e.g.:
# DATABASE_URL=postgres://USER:STRONGPASS@db-host:5432/zksl?sslmode=require

TZ=UTC
LC_ALL=C
LANG=C
NO_COLOR=1
FORCE_COLOR=

# Orchestrator security
API_KEY=change-me
# Or multiple keys (comma-separated)
# API_KEYS=key1,key2
RATELIMIT_MAX=60
RATELIMIT_WINDOW_MS=60000
IDEMP_MAX_ENTRIES=10000
LOG_LEVEL=info
